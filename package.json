{
  "name": "ssr-shelfish",
  "version": "1.0.0",
  "description": "Shelfish App",
  "main": "src/index.js",
  "author": "snow-dev",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "enzyme": "^3.9.0",
    "express": "^4.16.4",
    "immutable": "^4.0.0-rc.12",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-autobind": "^1.0.6",
    "react-dom": "^16.8.6",
    "react-localize-redux": "^3.5.2",
    "react-redux": "^7.0.2",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/resources -o src/resources",
    "watch-css": "npm run build-css && node-sass-chokidar src/resources -o src/resources --watch --recursive --usePolling",
    "start-js": "webpack-dev-server --open --mode development",
    "start": "npm-run-all -p watch-css start-js",
    "start:testing": "env-cmd .env.testing npm run start",
    "build": "webpack --mode production",
    "dev:build-server": "NODE_ENV=development webpack --config webpack.config.js --mode=development -w",
    "dev:start": "nodemon  ./server_build/index.js",
    "dev": "npm-run-all --parallel dev:*",
    "unit:test": "jest --watch",
    "unit:test:coverage": "jest --coverage"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "env-cmd": "^8.0.2",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "node-sass": "^4.11.0",
    "node-sass-chokidar": "^1.3.4",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "^16.8.6",
    "redux-devtools-extension": "^2.13.8",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/dist/**",
      "!**/vendor/**",
      "!**/server/**",
      "!**/public/**"
    ]
  }
}
